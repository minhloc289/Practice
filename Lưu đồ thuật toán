}

#Bai8 
#include <iostream>

using namespace std;
int main(){
    int n;
    do {
        cin >> n;
        if (n < 1){
            cout << "Nhap lai";
        }
    } while (n < 1);
    float S = 0;
    
    for (int i = 0; i < n; i++){
        S += (float)((2*i) + 1) / ((2*i) + 2);
    }
    cout << S;
    return 0;
}

